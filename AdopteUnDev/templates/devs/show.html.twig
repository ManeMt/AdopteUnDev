{% extends 'base.html.twig' %}

{% block title %}Developer{% endblock %}

{% block body %}
    <h1>Developer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ developer.id }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ developer.firstName }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ developer.lastName }}</td>
            </tr>
            <tr>
                <th>MinSalary</th>
                <td>{{ developer.minSalary }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ developer.level }}</td>
            </tr>
            <tr>
                <th>Biography</th>
                <td>{{ developer.biography }}</td>
            </tr>
            <tr>
                <th>Avatar</th>
                <td>{{ developer.avatar }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_developer_index') }}">back to list</a>

    <a href="{{ path('app_developer_edit', {'id': developer.id}) }}">edit</a>

    {{ include('devs/_delete_form.html.twig') }}
{% endblock %}
